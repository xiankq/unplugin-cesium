{
  "name": "unplugin-cesium",
  "type": "module",
  "version": "2.1.1",
  "description": "Quickly integrate CesiumJS into various bundlers",
  "license": "MIT",
  "homepage": "https://github.com/xiankq/unplugin-cesium#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiankq/unplugin-cesium.git"
  },
  "bugs": {
    "url": "https://github.com/xiankq/unplugin-cesium/issues"
  },
  "keywords": [
    "cesium",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint . --fix",
    "start": "esno src/index.ts",
    "test": "vitest",
    "taze": "taze -w -r major minor patch",
    "release": "bumpp -r",
    "============[CI]==========": "",
    "prepublish": "npm run build",
    "publish:ci": "npm publish -r --no-git-checks --access public"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": ">=5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "magic-string": "^0.30.17",
    "unplugin": "^2.1.2",
    "unplugin-copy": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@nuxt/kit": "^3.15.2",
    "@nuxt/schema": "^3.15.2",
    "@types/node": "^22.10.7",
    "@types/serve-static": "^1.15.7",
    "bumpp": "^9.10.1",
    "chalk": "^5.4.1",
    "eslint": "^9.18.0",
    "esno": "^4.8.0",
    "fast-glob": "^3.3.3",
    "nodemon": "^3.1.9",
    "rollup": "^4.31.0",
    "taze": "^18.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vite": "^5.4.13",
    "vitest": "^3.0.2",
    "webpack": "^5.97.1"
  }
}
